<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.goldencis.tdp.core.mapper.NavigationMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.goldencis.tdp.core.entity.Navigation">
        <id column="id" property="id" />
        <result column="compositor" property="compositor" />
        <result column="title" property="title" />
        <result column="url" property="url" />
        <result column="parent_id" property="parentId" />
        <result column="icon_url" property="iconUrl" />
        <result column="n_level" property="nLevel" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, compositor, title, url, parent_id, icon_url, n_level
    </sql>

</mapper>
